#include <iostream>
#include <string>
#include <cstdlib>
#include <ctime>

//Flame writed at 11:50 19/10/28

using namespace std ;

int main()
{
    cout<<"Multiplication table:"<<endl;
    
    int i, j;
    int x = 0;
    for(i = 1; i <= 9; i++){
        for(j = 1; j <= 9; j++){
            x = i * j;
            cout<<i<<"*"<<j<<"="<<x<<" ";
        }
        cout<<endl;
    }
    
    cout<<endl<<"Random integer: ";
    
    int rd;
    
    srand(time(NULL));
    rd = rand();
    cout<<rd;
    
    return 0;
}
